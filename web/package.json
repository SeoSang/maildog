{
  "name": "maildog",
  "version": "1.0.0",
  "scripts": {
    "dev": "set NODE_ENV=dev && next dev",
    "build": "set NODE_ENV=produciton && tsc --project tsconfig.json && next build",
    "start": "set NODE_ENV=production && next start",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "lint:fix": "yarn lint -- --fix",
    "prettier": "prettier '**/*.{json,yaml,md}' --check",
    "prettier:fix": "prettier '**/*.{json,yaml,md}' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint",
      "prettier"
    ]
  },
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  },
  "dependencies": {
    "@babel/core": "^7.17.8",
    "@chakra-ui/css-reset": "^1.1.3",
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.7",
    "@chakra-ui/system": "^1.12.0",
    "@egjs/react-grid": "^1.9.1",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@typescript-eslint/parser": "^5.17.0",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "cookie": "^0.4.2",
    "core-js": "^3.21.1",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "framer-motion": "^6.2.8",
    "http-status": "^1.5.0",
    "http-status-codes": "^2.2.0",
    "jotai": "^1.6.1",
    "knex": "^0.95.15",
    "next": "^12.1.4",
    "next-cookies": "^2.0.3",
    "nodemailer": "^6.7.3",
    "nodemailer-smtp-transport": "^2.7.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^18.0.0",
    "rooks": "^5.11.0",
    "sqlite3": "^5.0.2",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@swc-node/jest": "^1.4.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.4.1",
    "@types/crypto-js": "^4.1.1",
    "@types/http-status": "^1.1.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemailer-smtp-transport": "^2.7.5",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "babel-jest": "^27.5.1",
    "babel-plugin-styled-components": "^1.13.3",
    "eslint": "^8.12.0",
    "eslint-config-next": "^12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-yceffort": "0.0.5",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^6.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^11.2.6",
    "prettier": "2.4.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.6.3"
  }
